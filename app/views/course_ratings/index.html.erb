<p id="notice"><%= notice %></p>

<h1>Course Ratings</h1>

<table>
  <thead>
    <tr>
      <th>Review</th>
      <th>Cat1</th>
      <th>Cat2</th>
      <th>Cat3</th>
      <th>Cat4</th>
      <th>Cat5</th>
      <th>Content</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @course_ratings.each do |course_rating| %>
      <tr>
        <td><%= course_rating.review_id %></td>
        <td><%= course_rating.cat1 %></td>
        <td><%= course_rating.cat2 %></td>
        <td><%= course_rating.cat3 %></td>
        <td><%= course_rating.cat4 %></td>
        <td><%= course_rating.cat5 %></td>
        <td><%= course_rating.content %></td>
        <td><%= link_to 'Show', course_rating %></td>
        <td><%= link_to 'Edit', edit_course_rating_path(course_rating) %></td>
        <td><%= link_to 'Destroy', course_rating, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Course Rating', new_course_rating_path %>
