<p id="notice"><%= notice %></p>

<h1>General Courses</h1>

<table>
  <thead>
    <tr>
      <th>Course code</th>
      <th>Course title</th>
      <th>Course description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @general_courses.each do |general_course| %>
      <tr>
        <td><%= general_course.course_code %></td>
        <td><%= general_course.course_title %></td>
        <td><%= general_course.course_description %></td>
        <td><%= link_to 'Show', general_course %></td>
        <td><%= link_to 'Edit', edit_general_course_path(general_course) %></td>
        <td><%= link_to 'Destroy', general_course, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New General Course', new_general_course_path %>
